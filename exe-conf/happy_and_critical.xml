<?xml version="1.0" encoding="UTF-8"?>
<suite name="saucedemo test suite" parallel="none" thread-count="0">

	<parameter name="cucumber.filter.tags" value="@happy and @critical" />

	<listeners>
		<listener class-name="com.saucedemo.listener.RetryListener" />
	</listeners>

	<test name="saucedemo happy and critical scenarios">
		<classes>
			<class name="com.saucedemo.runner.TestRunner" />
		</classes>
	</test>
</suite>

